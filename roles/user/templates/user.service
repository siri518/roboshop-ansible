[Unit]
Description = Catalogue Service

[Service]
User=roboshop
Environment=MONGO=true
Environment=REDIS_URL='redis://redis-{{ env }}.venkysiridevops.online:6379'
Environment=MONGO_URL="mongodb://mongodb-{{ env }}.venkysiridevops.online:27017/catalogue"
ExecStart=/bin/node /app/server.js
SyslogIdentifier=catalogue

[Install]
WantedBy=multi-user.target